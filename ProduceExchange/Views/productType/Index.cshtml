@model IEnumerable<ProduceExchange.Models.productTypeModels>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
        @Html.ActionLink("Create New", "Create") | @Html.ActionLink("Return to Inventory Home", "Home", "Inventory")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.setProductTypes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.typeDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.setProductTypes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.typeDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
